{
  "name": "opencv4nodejs-lambda-layer",
  "version": "1.0.0",
  "description": "AWS Lambda layer for opencv4nodejs",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "package-opencv4nodejs-lambda-layer": "bash package.bash"
  },
  "opencv4nodejs": {
    "autoBuildFlags": "-DCMAKE_BUILD_TYPE=RELEASE -DBUILD_LIST=core,calib3d -DWITH_IPP=OFF -DWITH_V4L=OFF -DWITH_FFMPEG=OFF -DWITH_GSTREAMER=OFF -DWITH_GTK=OFF -DWITH_PROTOBUF=OFF -DBUILD_PROTOBUF=OFF -DOPENCV_DNN_OPENCL=OFF -DWITH_WIN32UI=OFF -DWITH_PNG=OFF -DWITH_JPEG=OFF -DWITH_TIFF=OFF -DWITH_WEBP=OFF -DWITH_OPENJPEG=OFF -DWITH_JASPER=OFF -DWITH_OPENEXR=OFF -DBUILD_opencv_python=OFF -DBUILD_opencv_java=OFF"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piercus/opencv4nodejs-lambda-layer.git"
  },
  "keywords": [
    "opencv",
    "opencv4nodejs",
    "layer",
    "lambda",
    "lambda-layer",
    "computervision",
    "cv"
  ],
  "dependencies": {
    "@u4/opencv4nodejs": "^6.4.1"
  },
  "author": "Pierre Colle <piercus@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/piercus/opencv4nodejs-lambda-layer/issues"
  },
  "homepage": "https://github.com/piercus/opencv4nodejs-lambda-layer#readme"
}
